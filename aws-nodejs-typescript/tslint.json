{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended",
    "tslint-eslint-rules"
  ],
  "jsRules": {},
  "rules": {
    "quotemark": [
      true,
      "single"
    ],
    "ter-indent": [
      true,
      2,
      {
        "SwitchCase": true
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "interface-name": false,
    "object-literal-sort-keys": false
  },
  "rulesDirectory": []
}
