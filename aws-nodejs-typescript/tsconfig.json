{
  "compilerOptions": {
    "sourceMap": true,
    "target": "es2017",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  },
  "exclude": [
    "node_modules"
  ]
}
